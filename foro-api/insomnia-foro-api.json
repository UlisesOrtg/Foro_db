      "parentId": "wrk_foroproject",
      "name": "Base Environment",
      "data": {
        "baseUrl": "http://localhost:8080",
        "token": ""
      }
    },
    {
      "_id": "fld_auth",
      "_type": "request_group",
      "parentId": "wrk_foroproject",
      "name": "Auth"
    },
    {
      "_id": "req_register",
      "_type": "request",
      "parentId": "fld_auth",
      "name": "POST /auth/register",
      "method": "POST",
      "url": "{{ _.baseUrl }}/auth/register",
      "body": {
        "mimeType": "application/json",
        "text": "{\"nombre\": \"Demo\", \"email\": \"nuevo@foro.com\", \"password\": \"demo123\"}"
      }
    },
    {
      "_id": "req_login",
      "_type": "request",
      "parentId": "fld_auth",
      "name": "POST /auth/login",
      "method": "POST",
      "url": "{{ _.baseUrl }}/auth/login",
      "body": {
        "mimeType": "application/json",
        "text": "{\"email\": \"demo@foro.com\", \"password\": \"demo123\"}"
      }
    },
    {
      "_id": "fld_topicos",
      "_type": "request_group",
      "parentId": "wrk_foroproject",
      "name": "Topicos"
    },
    {
      "_id": "req_listar",
      "_type": "request",
      "parentId": "fld_topicos",
      "name": "GET /topicos (paginado)",
      "method": "GET",
