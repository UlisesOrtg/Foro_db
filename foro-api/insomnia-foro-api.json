      },
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      }
    },
    {
      "_id": "req_eliminar",
      "_type": "request",
      "parentId": "fld_topicos",
      "name": "DELETE /topicos/{id}",
      "method": "DELETE",
      "url": "{{ _.baseUrl }}/topicos/1",
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      }
    },
    {
      "_id": "req_actualizar",
      "_type": "request",
      "parentId": "fld_topicos",
      "name": "PUT /topicos/{id}",
      "method": "PUT",
      "url": "{{ _.baseUrl }}/topicos/1",
      "body": {
        "mimeType": "application/json",
        "text": "{\"titulo\": \"Titulo actualizado\", \"mensaje\": \"Mensaje actualizado\", \"curso\": \"Java Avanzado\"}"
      },
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      }
    }
  ]
